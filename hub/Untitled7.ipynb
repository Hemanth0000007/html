    "n = [1 , 3 , 5]\n",
    "ans = 0\n",
    "for j in range(32):\n",
    "    cnt = 0\n",
    "    for i in n:\n",
    "        if i >> j & 1 == 1:\n",
    "            cnt+=1 \n",
    "    ans += 2*cnt*(len(n)-cnt)\n",
    "\n",
    "print(ans)"
   ]
